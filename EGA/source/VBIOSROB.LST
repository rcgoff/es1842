 0304  22 E4                 C		AND	AH,AH			; READING ZERO PATTERN THIS PASS ? 
 0306  75 04                 C		JNZ	C6			; CONTINUE TEST SEQUENCE TILL 0 
 0308  8A D4                 C		MOV	DL,AH			;  ELSE SET 0 FOR END READ PATTERN 
