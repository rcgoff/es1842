			     C	 
			     C	;----- SCROLL REQUIRED 
			     C	 
 1D39                        C	U1: 
 1D39  E8 115E R             C		CALL	SET_CPOS			; SET THE CURSOR 
			     C	 
			     C	;----- DETERMINE VALUE TO FILL WITH DURING SCROLL 
			     C	 
 1D3C  A0 0449 R             C		MOV	AL,CRT_MODE			; GET THE CURRENT MODE 
 1D3F  3C 04                 C		CMP	AL,4 
