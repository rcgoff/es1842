			     C	;---------------------------------------------------------------- 
			     C	; LIGHT PEN							: 
			     C	;	THIS ROUTINE TESTS THE LIGHT PEN SWITCH AND THE LIGHT	: 
			     C	;	PEN TRIGGER. IF BOTH ARE SET, THE LOCATION OF THE LIGHT : 
			     C	;	PEN IS DETERMINED. OTHERWISE, A RETURN WITH NO		: 
			     C	;	INFORMATION IS MADE.					: 
			     C	; ON EXIT							: 
			     C	;	(AH) = 0 IF NO LIGHT PEN INFORMATION IS AVAILABLE	: 
			     C	;		 BX,CX,DX ARE DESTROYED				: 
			     C	;	(AH) = 1 IF LIGHT PEN IS AVAILABLE			: 
			     C	;		 (DH,DL) = ROW,COLUMN OF CURRENT LIGHT PEN	: 
			     C	;			   POSITION				: 
			     C	;		 (CH) = RASTER POSITION (OLD MODES)		: 
			     C	;		 (CX) = RASTER POSITION (NEW MODES)		: 
			     C	;		 (BX) = BEST GUESS AT PIXEL HORIZONTAL POSITION : 
			     C	;---------------------------------------------------------------- 
			     C		ASSUME	CS:CODE,DS:ABS0 
			     C	;----- SUBTRACT_TABLE 
 11C2                        C	V1	LABEL	BYTE 
 11C2  06 06 07 07 05 05     C		DB	006H,006H,007H,007H,005H,005H	; 0-5 
 11C8  04 05 00 00 00 00     C		DB	004H,005H,000H,000H,000H,000H	; 6-B 
 11CE  00 05 06 04 04 04     C		DB	000H,005H,006H,004H,004H,004H	; C-11 
 11D4  04 06 06 04 07 04     C		DB	004H,006H,006H,004H,007H,004H	; 12-17 
 11DA  07 04                 C		DB	007H,004H			; 18-19 
			     C	 
 11DC                        C	READ_LPEN	PROC	NEAR 
			     C	 
			     C	;----- WAIT FOR LIGHT PEN TO BE DEPRESSED 
			     C	 
 11DC  8B 16 0463 R          C		MOV	DX,ADDR_6845			; GET BASE ADDRESS OF 6845 
 11E0  83 C2 06              C		ADD	DX,6				; POINT TO STATUS REGISTER 
 11E3  EC                    C		IN	AL,DX				; GET STATUS REGISTER 
 11E4  A8 04                 C		TEST	AL,4				; TEST LIGHT PEN SWITCH 
 11E6  B4 00                 C		MOV	AH,0				; SET NO LIGHT PEN RETURN 
 11E8  74 03                 C		JZ	V9				;  CODE 
 11EA  E9 1292 R             C		JMP	V6				; NOT SET, RETURN 
			     C	 
			     C	;----- NOW TEST FOR LIGHT PEN TRIGGER 
			     C	 
 11ED                        C	V9: 
 11ED  A8 02                 C		TEST	AL,2				; TEST LIGHT PEN TRIGGER 
 11EF  75 03                 C		JNZ	V7A				; RETURN WITHOUT RESETTING 
			     C							;  TRIGGER 
 11F1  E9 129C R             C		JMP	V7				; EXIT LIGHT PEN ROUTINE 
			     C	 
			     C	;----- TRIGGER HAS BEEN SET, READ THE VALUE IN 
			     C	 
 11F4                        C	V7A: 
			     C	 
 11F4  B4 10                 C		MOV	AH,16				; LIGHT PEN REGISTERS 
			     C	 
			     C	;----- INPUT REGS POINTED TO BY AH, AND CONVERT TO ROW COLUMN IN DX 
			     C	 
 11F6  8B 16 0463 R          C		MOV	DX,ADDR_6845			; ADDRESS REGISTER 
 11FA  8A C4                 C		MOV	AL,AH				; REGISTER TO READ 
 11FC  EE                    C		OUT	DX,AL				; SET IT UP 
 11FD  42                    C		INC	DX				; DATA REGISTER 
 11FE  50                    C		PUSH	AX 
 11FF  EC                    C		IN	AL,DX				; GET THE VALUE 
 1200  8A E8                 C		MOV	CH,AL				; SAVE IN CX 
 1202  58                    C		POP	AX 
 1203  4A                    C		DEC	DX				; ADDRESS REGISTER 
 1204  FE C4                 C		INC	AH 
 1206  8A C4                 C		MOV	AL,AH				; SECOND DATA REGISTER 
 1208  EE                    C		OUT	DX,AL 
 1209  42                    C		INC	DX				; POINT TO DATA REGISTER 
 120A  EC                    C		IN	AL,DX				; GET THE 2ND DATA VALUE 
 120B  8A E5                 C		MOV	AH,CH				; AX HAS INPUT VALUE 
			     C	 
			     C	;----- AX HAS THE VALUE READ IN FROM THE 6845 
			     C	 
 120D  8A 1E 0449 R          C		MOV	BL,CRT_MODE 
 1211  2A FF                 C		SUB	BH,BH				; MODE VALUE TO BX 
 1213  2E: 8A 9F 11C2 R      C		MOV	BL,CS:V1[BX]			; AMOUNT TO SUBTRACT 
 1218  2B C3                 C		SUB	AX,BX				; TAKE IT AWAY 
 121A  8B 1E 044E R          C		MOV	BX,CRT_START			; SCREEN ADDRESS 
 121E  D1 EB                 C		SHR	BX,1				; DIVIDE BY 2 
 1220  2B C3                 C		SUB	AX,BX				; ADJUST TO ZERO START 
 1222  79 02                 C		JNS	V2				; IF POSITIVE, GET MODE 
 1224  2B C0                 C		SUB	AX,AX				; <0 PLAYS AS 0 
			     C	 
			     C	;----- DETERMINE MODE OF OPERATION 
			     C	 
 1226                        C	V2:						; DETERMINE_MODE 
 1226  B1 03                 C		MOV	CL,3				; SET *8 SHIFT COUNT 
 1228  80 3E 0449 R 04       C		CMP	CRT_MODE,4			; GRAPHICS OR ALPHA 
 122D  72 4D                 C		JB	V4				; ALPHA_PEN 
 122F  80 3E 0449 R 07       C		CMP	CRT_MODE,7 
 1234  74 46                 C		JE	V4				; ALPHA_PEN 
			     C	 
 1236  80 3E 0449 R 06       C		CMP	CRT_MODE,06H 
 123B  77 28                 C		JA	V8 
 123D  75 02                 C		JNE	V8X 
 123F  D1 E8                 C		SHR	AX,1 
			     C	 
			     C	;----- OLD GRAPHICS MODES 
			     C	 
 1241                        C	V8X: 
 1241  B2 28                 C		MOV	DL,40				; DIVISOR FOR GRAPHICS 
 1243  F6 F2                 C		DIV	DL				; ROW(AL) AND COLUMN(AH) 
			     C							;  AL RANGE 0-99, 
			     C							;  AH RANGE 0-39 
			     C	;----- DETERMINE GRAPHIC ROW POSITION 
			     C	 
 1245  8A E8                 C		MOV	CH,AL				; SAVE ROW VALUE IN CH 
 1247  02 ED                 C		ADD	CH,CH				; *2 FOR EVEN/ODD FIELD 
 1249  8A DC                 C		MOV	BL,AH				; COLUMN VALUE TO BX 
 124B  2A FF                 C		SUB	BH,BH				; *8 FOR MEDIUM RES 
 124D  80 3E 0449 R 06       C		CMP	CRT_MODE,6			; MEDIUM OR HIGH RES 
 1252  75 04                 C		JNE	V3				; NOT_HIGH_RES 
 1254  B1 04                 C		MOV	CL,4				; SHIFT VALUE FOR HIGH RES 
 1256  D0 E4                 C		SAL	AH,1				; COLUMN VALUE *2 FOR HIGH RES 
 1258                        C	V3:						; NOT_HIGH_RES 
 1258  D3 E3                 C		SHL	BX,CL				; *16 FOR HIGH RES 
			     C	 
			     C	;----- DETERMINE ALPHA CHAR POSITION 
			     C	 
 125A  8A D4                 C		MOV	DL,AH				; COLUMN VALUE FOR RETURN 
 125C  8A F0                 C		MOV	DH,AL				; ROW VALUE 
 125E  D0 EE                 C		SHR	DH,1				; DIVIDE BY 4 
 1260  D0 EE                 C		SHR	DH,1				; FOR VALUE IN 0-24 RANGE 
 1262  EB 2C 90              C		JMP	V5				; LIGHT_PEN_RETURN_SET 
 1265                        C	V8: 
			     C	 
			     C	;----- NEW GRAPHICS MODES 
			     C	 
 1265  99                    C		CWD					; PREPARE TO DIVIDE 
 1266  F7 36 044A R          C		DIV	CRT_COLS			; AX = ROW, DX = COLUMN 
 126A  8B DA                 C		MOV	BX,DX				; SAVIE REMAINDER 
 126C  D3 E3                 C		SAL	BX,CL				; PEL COLUMN 
 126E  8B C8                 C		MOV	CX,AX				; PEL ROW 
 1270  52                    C		PUSH	DX				; SAVE FROM DIVIDE 
 1271  99                    C		CWD					; PREPARE TO DIVIDE 
 1272  F7 36 0485 R          C		DIV	POINTS				; DIVIDE BY BYTES/CHAR 
 1276  5A                    C		POP	DX				; RECOVER 
 1277  8A F0                 C		MOV	DH,AL				; CHARACTER ROW 
 1279  EB 15 90              C		JMP	V5 
			     C	 
			     C	;----- ALPHA MODE ON LIGHT PEN 
 127C                        C	V4:						; ALPHA PEN 
 127C  F6 36 044A R          C		DIV	BYTE PTR CRT_COLS		; ROW,COLUMN VALUE 
 1280  8A F0                 C		MOV	DH,AL				; ROWS TO DH 
 1282  8A D4                 C		MOV	DL,AH				; COLS TO DL 
 1284  8A DC                 C		MOV	BL,AH				; COLUMN VALUE 
 1286  32 FF                 C		XOR	BH,BH				;  TO BX 
 1288  D3 E3                 C		SAL	BX,CL 
 128A  F6 26 0485 R          C		MUL	BYTE PTR POINTS 
 128E  8B C8                 C		MOV	CX,AX 
 1290                        C	V5:						; LIGHT_PEN_RETURN_SET 
 1290  B4 01                 C		MOV	AH,1				; INDICATE EVERTHING SET 
 1292                        C	V6:						; LIGHT_PEN_RETURN 
 1292  52                    C		PUSH	DX				; SAVE RETURN VALUE 
			     C							;  (IN CASE) 
 1293  8B 16 0463 R          C		MOV	DX,ADDR_6845			; GET BASE ADDRESS 
 1297  83 C2 07              C		ADD	DX,7				; POINT TO RESET PARM 
 129A  EE                    C		OUT	DX,AL				; ADDRESS, NOT DATA, 
			     C							;  IS IMPORTANT 
 129B  5A                    C		POP	DX				; RECOVER VALUE 
 129C                        C	V7:						; RETURN_NO_RESET 
 129C  5F                    C		POP	DI 
 129D  5E                    C		POP	SI 
 129E  83 C4 06              C		ADD	SP,6				; DISCARD SAVED BX,CX,DX 
 12A1  1F                    C		POP	DS 
 12A2  07                    C		POP	ES 
 12A3  5D                    C		POP	BP 
 12A4  CF                    C		IRET 
 12A5                        C	READ_LPEN	ENDP 
