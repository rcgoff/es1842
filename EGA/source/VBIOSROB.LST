 14BA  8A C7                 C		MOV	AL,BH				; ATTRIBUTE TO FILL WITH 
 14BC                        C	R10: 
 14BC  E8 14E7 R             C		CALL	R18				; CLEAR THAT ROW 
 14BF  81 EF 1FB0            C		SUB	DI,2000H-80			; POINT TO NEXT LINE 
 14C3  FE CB                 C		DEC	BL				; NUMBER OF LINES TO FILL 
 14C5  75 F5                 C		JNZ	R10				; CLEAR_LOOP 
 14C7  E9 219E R             C		JMP	V_RET 
