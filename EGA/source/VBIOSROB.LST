 1494  1F                    C		POP	DS				;  POINTING TO REGEN 
 1495  2A ED                 C		SUB	CH,CH				; 0 TO HIGH OF COUNT REG 
 1497  D0 E3                 C		SAL	BL,1				; NUMBER OF LINES *4 
 1499  D0 E3                 C		SAL	BL,1 
 149B  74 2D                 C		JZ	R11				; IF 0, BLANK ENTIRE FIELD 
 149D  8A C3                 C		MOV	AL,BL				; NUMBER OF LINES IN AL 
 149F  B4 50                 C		MOV	AH,80				; 80 BYTES/ROW 
 14A1  F6 E4                 C		MUL	AH				; OFFSET TO SOURCE 
 14A3  8B F7                 C		MOV	SI,DI				; SET UP SOURCE 
 14A5  03 F0                 C		ADD	SI,AX				;  ADD IN OFFSET TO IT 
 14A7  8A E6                 C		MOV	AH,DH				; NUMBER OF ROWS IN FIELD 
 14A9  2A E3                 C		SUB	AH,BL				; DETERMINE NUMBER TO MOVE 
			     C	 
			     C	;---- LOOP THROUGH, MOVING ONE ROW AT A TIME, BOTH EVEN AND ODD FIELDS 
			     C	 
 14AB                        C	R8:						; ROW_LOOP 
 14AB  E8 14CE R             C		CALL	R17				; MOVE ONE ROW 
 14AE  81 EE 1FB0            C		SUB	SI,2000H-80			; MOVE TO NEXT ROW 
 14B2  81 EF 1FB0            C		SUB	DI,2000H-80 
 14B6  FE CC                 C		DEC	AH				; NUMBER OF ROWS TO MOVE 
 14B8  75 F1                 C		JNZ	R8				; CONTINUE TILL ALL MOVED 
			     C	 
