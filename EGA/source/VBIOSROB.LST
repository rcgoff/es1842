			     C	; SCROLL_UP							: 
			     C	;	THIS ROUTINE SCROLLS UP THE INFORMATION ON THE CRT	: 
			     C	; ENTRY								: 
			     C	;	CH,CL = UPPER LEFT CORNER OF REGION TO SCROLL		: 
			     C	;	DH,DL = LOWER RIGHT CORNER OF REGION TO SCROLL		: 
			     C	;	 BOTH OF THE ABOVE ARE IN CHARACTER POSITIONS		: 
			     C	;	BH = FILL VALUE FOR BLANKED LINES			: 
			     C	;	AL = # LINES TO SCROLL (AL=0 MEANS BLANK THE ENTIRE	: 
			     C	;	     FIELD)						: 
			     C	;	DS = DATA SEGMENT					: 
			     C	;	ES = REGEN SEGMENT					: 
			     C	; EXIT								: 
			     C	;	NOTHING, THE SCREEN IS SCROLLED				: 
			     C	;---------------------------------------------------------------- 
 1475                        C	GRAPHICS_UP	PROC	NEAR 
 1475  8A D8                 C		MOV	BL,AL				; SAVE LINE COUNT IN BL 
 1477  8B C1                 C		MOV	AX,CX				; GET UPPER LEFT POSITION 
			     C							;  INTO AX REG 
			     C	 
			     C	;----- USE CHARACTER SUBROUTINE FOR POSITIONING 
			     C	;----- ADDRESS RETURNED IS MULTIPLIED BY 2 FROM CORRECT VALUE 
			     C	 
 1479  E8 16A8 R             C		CALL	GRAPH_POSN 
 147C  8B F8                 C		MOV	DI,AX				; SAVE RESULT AS 
			     C							;  DESTINATION ADDRESS 
			     C	 
			     C	;----- DETERMINE SIZE OF WINDOW 
			     C	 
 147E  2B D1                 C		SUB	DX,CX 
 1480  81 C2 0101            C		ADD	DX,101H				; ADJUST VALUES 
 1484  D0 E6                 C		SAL	DH,1				; MULTIPLY # ROWS BY 4 
			     C							; SINCE 8 VERT DOTS/CHAR 
 1486  D0 E6                 C		SAL	DH,1				; AND EVEN/ODD ROWS 
			     C	 
			     C	;----- DETERMINE CRT MODE 
			     C	 
 1488  80 3E 0449 R 06       C		CMP	CRT_MODE,6			; TEST FOR MEDIUM RES 
 148D  73 04                 C		JNC	R7				; FIND_SOURCE 
			     C	 
