			     C	;---------------------------------------------------------------- 
			     C	; THIS ROUTINE PERFORMS A READ/WRITE TEST ON A BLOCK OF STORAGE : 
			     C	; (MAX SIZE = 32KW).  IF "WARM START", FILL BLOCK WITH 0000 AND : 
			     C	; RETURN.							: 
			     C	; ON ENTRY:							: 
			     C	;   ES = ADDRESS OF STORAGE TO BE TESTED			: 
			     C	;   DS = ADDRESS OF STORAGE TO BE TESTED			: 
			     C	;   CX = WORD COUNT OF STORAGE BLOCK TO BE TESTED		: 
			     C	;	(MAX. = 8000H (32K WORDS))				: 
			     C	; ON EXIT:							: 
			     C	;   ZERO FLAG = OFF IF STORAGE ERROR				: 
			     C	; AX,BX,CX,DX,DI,SI ARE ALL DESTROYED.				: 
			     C	;---------------------------------------------------------------- 
 05FC                        C	PODSTG	PROC	NEAR 
 05FC  55                    C		PUSH	BP 
 05FD  FC                    C		CLD					; SET DIR TO INCREMENT 
 05FE  2B FF                 C		SUB	DI,DI				; SET DI=0000 REL TO START 
			     C							;  OF SEGMENT 
 0600  2B C0                 C		SUB	AX,AX				; INITIAL DATA PATTERN FOR 
			     C							;  00-FF TEST 
 0602  E8 0CFF R             C		CALL	DDS 
			     C		ASSUME	DS:ABS0 
 0605  8B 1E 0472 R          C		MOV	BX,DS:RESET_FLAG		; WARM START? 
 0609  81 FB 1234            C		CMP	BX,1234H 
 060D  8C C2                 C		MOV	DX,ES 
 060F  8E DA                 C		MOV	DS,DX				; RESTORE DS 
 0611  74 62                 C		JE	PODSTG_5			; GO DO FILL WITH 0000 
			     C							;  IF WARM START 
 0613  81 FB 4321            C		CMP	BX,4321H			; DCP WARM START? 
 0617  74 5C                 C		JE	PODSTG_5			; DO FILL IF SO 
 0619                        C	PODSTG_1: 
 0619  88 05                 C		MOV	[DI],AL				; WRITE TEST DATA 
 061B  8A 05                 C		MOV	AL,[DI]				; GET IT BACK 
 061D  32 C4                 C		XOR	AL,AH				; COMPARE TO EXPECTED 
 061F  75 40                 C		JNZ	PODSTG_ERRO			; ERROR EXIT IF MISCOMPARE 
 0621  FE C4                 C		INC	AH				; FORM NEW DATA PATTERN 
 0623  8A C4                 C		MOV	AL,AH 
 0625  75 F2                 C		JNZ	PODSTG_1			; LOOP TILL ALL 256 DATA 
			     C							;  PATTERNS DONE 
 0627  8B E9                 C		MOV	BP,CX				; SAVE WORD COUNT 
 0629  B8 AA55               C		MOV	AX,0AA55H			; LOAD DATA PATTERN 
 062C  8B D8                 C		MOV	BX,AX 
 062E  BA 55AA               C		MOV	DX,055AAH			; LOAD OTHER DATA PATTERN 
 0631  F3/ AB                C		REP	STOSW				; FILL WORDS FROM LOW TO 
			     C							;  HIGH WITH AAAA 
 0633  4F                    C		DEC	DI				; POINT TO LAST WORD 
 0634  4F                    C		DEC	DI				;  WRITTEN 
 0635  FD                    C		STD					; SET DIR FLAG TO GO DOWN 
 0636  8B F7                 C		MOV	SI,DI				; SET INDEX REGS. EQUAL 
 0638  8B CD                 C		MOV	CX,BP				; RECOVER WORD COUNT 
 063A                        C	PODSTG_2:					; GO FROM HIGH TO LOW 
 063A  AD                    C		LODSW					; GET WORD FROM MEMORY 
 063B  33 C3                 C		XOR	AX,BX				; EQUAL WHAT S/B THERE? 
 063D  75 22                 C		JNZ	PODSTG_ERRO			; GO ERROR EX IT IF NOT 
 063F  8B C2                 C		MOV	AX,DX				; GET 55 DATA PATTERN AND 
 0641  AB                    C		STOSW					;  STORE IN LOC JUST READ 
 0642  E2 F6                 C		LOOP	PODSTG_2			; LOOP TILL ALL BYTES DONE 
 0644  8B CD                 C		MOV	CX,BP				; RECOVER WORD COUNT 
 0646  FC                    C		CLD					; BACK TO INCREMENT 
 0647  46                    C		INC	SI				; ADJUST PTRS 
 0648  46                    C		INC	SI 
 0649  8B FE                 C		MOV	DI,SI 
 064B                        C	PODSTG_3:					; LOW TO HIGH DOING WORDS 
 064B  AD                    C		LODSW					; GET A WORD 
 064C  33 C2                 C		XOR	AX,DX				; SHOULD COMPARE TO DX 
 064E  75 11                 C		JNZ	PODSTG_ERRO			; GO ERROR IF NOT 
 0650  AB                    C		STOSW					; WRITE 0000 BACK TO LOC 
			     C							;  JUST READ 
 0651  E2 F8                 C		LOOP	PODSTG_3			; LOOP TILL DONE 
			     C	 
 0653  FD                    C		STD					; BACK TO DECREMENT 
 0654  4E                    C		DEC	SI				; ADJUST POINTER DOWN TO 
			     C							;  LAST WORD WRITTEN 
 0655  4E                    C		DEC	SI 
 0656  8B CD                 C		MOV	CX,BP				; GET WORD COUNT 
 0658                        C	PODSTG_4: 
 0658  AD                    C		LODSW					; GET WORD 
 0659  0B C0                 C		OR	AX,AX				; = TO 0000 
 065B  75 04                 C		JNZ	PODSTG_ERRO			; ERROR IF NOT 
 065D  E2 F9                 C		LOOP	PODSTG_4			; LOOP TILL DONE 
 065F  EB 11                 C		JMP	SHORT PODSTG_ERR2 
 0661                        C	PODSTG_ERRO: 
 0661  8B C8                 C		MOV	CX,AX				; SAVE BITS IN ERROR 
 0663  32 E4                 C		XOR	AH,AH 
 0665  0A ED                 C		OR	CH,CH				; HIGH BYTE ERROR? 
 0667  74 02                 C		JZ	PODSTG_ERR1 
 0669  B4 01                 C		MOV	AH,1				; SET HIGH BYTE ERROR 
 066B                        C	PODSTG_ERR1: 
 066B  0A C9                 C		OR	CL,CL				; LOW BYTE ERROR? 
 066D  74 03                 C		JZ	PODSTG_ERR2 
 066F  80 C4 02              C		ADD	AH,2 
 0672                        C	PODSTG_ERR2: 
 0672  5D                    C		POP	BP 
 0673  FC                    C		CLD					; SET DIR FLAG BACK TO INC 
 0674  C3                    C		RET					; RETURN TO CALLER 
 0675                        C	PODSTG_5:					; SIMPLE FILL WITH 0000 ON 
			     C							;  WARM-START 
 0675  50                    C		PUSH	AX				; SAVE 
 0676  52                    C		PUSH	DX				; SAVE VALUE 
 0677  B6 03                 C		MOV	DH,3 
 0679  B2 C4                 C		MOV	DL,SEQ_ADDR			; SEQ_ADDR  REGISTER 
 067B  B8 020F               C		MOV	AX,020FH 
 067E  E8 0D16 R             C		CALL	OUT_DX				; DO IT 
 0681  5A                    C		POP	DX				; RESTORE 
 0682  58                    C		POP	AX				; RESTORE 
 0683  F3/ AB                C		REP	STOSW 
 0685  E8 0CFF R             C		CALL	DDS 
			     C		ASSUME	DS:ABS0 
 0688  89 1E 0472 R          C		MOV	DS:RESET_FLAG,BX 
 068C  8E DA                 C		MOV	DS,DX				; RESTORE DS 
 068E  EB E2                 C		JMP	PODSTG_ERR2			; AND EXIT 
 0690                        C	PODSTG	ENDP 
