			     C	;---------------------------------------------------------------- 
			     C	; ACT_DISP_PAGE		SELECT ACTIVE DISPLAY PAGE		: 
			     C	;	THIS ROUTINE SETS THE ACTIVE DISPLAY PAGE, ALLOWING	: 
			     C	;	FOR MULTIPLE PAGES OF DISPLAYED VIDEO.			: 
			     C	; INPUT								: 
			     C	;	AL HAS THE NEW ACTIVE DISPLAY PAGE			: 
			     C	; OUTPUT							: 
			     C	;	THE CRTC IS RESET TO DISPLAY THAT PAGE			: 
			     C	;---------------------------------------------------------------- 
 12A5                        C	AH5: 
 12A5  A2 0462 R             C		MOV	ACTIVE_PAGE,AL			; SAVE ACTIVE PAGE VALUE 
 12A8  8B 0E 044C R          C		MOV	CX,CRT_LEN			; GET SAVED LENGTH OF 
			     C							;  REGEN BUFFER 
 12AC  98                    C		CBW					; CONVERT AL TO WORD 
 12AD  50                    C		PUSH	AX				; SAVE PAGE VALUE 
 12AE  F7 E1                 C		MUL	CX				; DISPLAY PAGE TIMES 
			     C							;  REGEN LENGTH 
 12B0  A3 044E R             C		MOV	CRT_START,AX			; SAVE START ADDRESS FOR 
			     C							;  LATER REQUIREMENTS 
 12B3  8B C8                 C		MOV	CX,AX				; START ADDRESS TO CX 
 12B5  8A 1E 0449 R          C		MOV	BL,CRT_MODE 
 12B9  80 FB 07              C		CMP	BL,7				; DO NOT DIVIDE BY TWO 
 12BC  77 02                 C		JA	ADP_1 
 12BE                        C	ADP_2: 
 12BE  D1 F9                 C		SAR	CX,1				; / 2 FOR CRTC HANDLING 
 12C0                        C	ADP_1: 
 12C0  B4 0C                 C		MOV	AH,C_STRT_HGH			; REG FOR START ADDRESS 
 12C2  E8 1136 R             C		CALL	M16 
 12C5  5B                    C		POP	BX				; RECOVER PAGE VALUE 
 12C6  D1 E3                 C		SAL	BX,1				; *2 FOR WORD OFFSET 
 12C8  8B 87 0450 R          C		MOV	AX,[BX + OFFSET CURSOR_POSN]	; GET CURSOR FOR THIS PAGE 
 12CC  E8 1176 R             C		CALL	M18				; SET THE CURSOR POSITION 
 12CF  E9 219E R             C		JMP	V_RET 
			     C	 
