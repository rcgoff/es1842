 0690                        C	HOW_BIG		PROC NEAR 
 0690  8C DA                 C		MOV	DX,DS				; SET PNTR TO BUFFER LOC 
 0692  2B DB                 C		SUB	BX,BX				; BASIC COUNT OF 00K 
 0694                        C	FILL_LOOP: 
 0694  8E C2                 C		MOV	ES,DX				; SET SEG. REG 
 0696  2B FF                 C		SUB	DI ,DI 
 0698  B8 AA55               C		MOV	AX,0AA55H			; TEST PATTERN 
 069B  8B C8                 C		MOV	CX,AX 
 069D  26: 89 05             C		MOV	ES:[DI],AX			; SEND TO MEMORY 
 06A0  B0 0F                 C		MOV	AL,0FH				; PUT SOMETHING IN AL 
 06A2  26: 8B 05             C		MOV	AX,ES:[DI]			; GET PATTERN FROM MEMORY 
 06A5  33 C1                 C		XOR	AX,CX				; COMPARE PATTERNS 
 06A7  75 14                 C		JNZ	HOW_BIG_END			; GO END IF NO COMPARE 
 06A9  B9 2000               C		MOV	CX,2000H			; SET COUNT FOR 8K WORDS 
 06AC  F3/ AB                C		REP	STOSW				; FILL 8K WORDS 
 06AE  81 C2 0400            C		ADD	DX,0400H			; POINT TO NEXT 16K BLOCK 
 06B2  83 C3 10              C		ADD	BX,16				; BUMP COUNT BY 16KB 
 06B5  80 FE B0              C		CMP	DH,0B0H 
 06B8  75 DA                 C		JNZ	FILL_LOOP			; AREA YET ?(B0000H) 
 06BA  EB 01 90              C		JMP	HOW_BIG_END 
 06BD                        C	HOW_BIG_END: 
 06BD  80 FE A0              C		CMP	DH,0A0H				; 1ST 16KB OK 
 06C0  74 06                 C		JZ	HB_ERROR_EXIT 
 06C2                        C	RESUME: 
 06C2  01 5E 04              C		ADD	WORD PTR[BP][4],BX		; SAVE BUFFER FOUND 
 06C5  B8 0000               C		MOV	AX,0 
 06C8                        C	HB_ERROR_EXIT: 
 06C8  C3                    C		RET 
 06C9                        C	HOW_BIG ENDP 
