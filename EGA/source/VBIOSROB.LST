 02ED  FD                    C		STD				; SET DIR FLAG TO GO BACKWARDS 
 02EE                        C	C4: 
 02EE  8B F7                 C		MOV	SI,DI 
 02F0  8B CB                 C		MOV	CX,BX			; SETUP BYTE CNT 
 02F2                        C	C5:					; INNER TEST LOOP 
 02F2  AC                    C		LODSB				; LOAD OLD TEST BYTE [SI]+ 
